{
  "name": "expo-bare-eas-labs",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "reset": "npm start -- --reset-cache",
    "lint": "eslint .",
    "ts:check": "tsc",
    "format": "prettier --write \"**/*.+(js|tsx|json|css|md|mdx)\"",
    "format:check": "prettier --check \"**/*.+(js|tsx|json|css|md|mdx)\"",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "xcode": "open ios/expobareeaslabs.xcworkspace/",
    "ipods": "cd ios && pod deintegrate && pod install && cd .."
  },
  "dependencies": {
    "expo": "^49.0.8",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "^2.4.8",
    "expo-device": "~5.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "graphql": "^15.8.0",
    "graphql-request": "^6.1.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-beacons-manager": "github:oalcantarhypha/react-native-beacons-manager#expo-bare",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-mmkv": "^2.10.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-ui-lib": "^7.8.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.2.18",
    "eslint": "^8.46.0",
    "eslint-config-universe": "^11.3.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  },
  "private": true
}
